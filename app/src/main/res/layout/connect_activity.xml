                android:textColor="@color/black"
                android:textSize="10dp"
                android:text="Consumption: "/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_consumption_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_consumption_title"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_consumption_title"
                android:gravity="center"
                android:textColor="@color/black"
                android:text="0000.000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
