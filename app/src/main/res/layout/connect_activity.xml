                app:layout_constraintEnd_toStartOf="@+id/recorded_data_none_content"
                app:layout_constraintHorizontal_chainStyle="packed"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="30dp"
                android:textColor="@color/black"
                android:textSize="10dp"
                android:text="None: "/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_none_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_none_title"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_none_title"
                android:gravity="center"
                android:textColor="@color/black"
                android:text="@string/fail"/>

        </androidx.constraintlayout.widget.ConstraintLayout>



        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/load_title"
            app:layout_constraintTop_toBottomOf="@+id/recorded_data_constrainlayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/load_content"
            android:layout_marginTop="20dp"
            app:layout_constraintHorizontal_chainStyle="packed"
            android:gravity="center"
