                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/recorded_data_start_time_content"
                app:layout_constraintHorizontal_chainStyle="packed"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:text="Start: "
                android:textSize="10dp"
                android:textColor="@color/black"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_start_time_content"

                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_start_time_title"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_start_time_title"
                app:layout_constraintEnd_toEndOf="parent"
                android:gravity="center"
                android:textSize="10dp"
                android:text="0000-00-00 00:00:00"
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_end_time_title"
                app:layout_constraintTop_toBottomOf="@+id/recorded_data_start_time_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/recorded_data_end_time_content"
                app:layout_constraintHorizontal_chainStyle="packed"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="10dp"
                android:text="End: "/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_end_time_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_end_time_title"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_end_time_title"
                app:layout_constraintEnd_toEndOf="parent"
