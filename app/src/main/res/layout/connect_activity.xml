            android:onClick="onClick"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/appearance_content"
            app:layout_constraintTop_toTopOf="@+id/device_name_title"
            app:layout_constraintBottom_toBottomOf="@+id/device_name_title"
            app:layout_constraintStart_toEndOf="@+id/appearance_title"
            app:layout_constraintEnd_toEndOf="parent"
            android:gravity="center"
            android:layout_marginStart="10dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:text="@string/fail"/>


        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/peripheral_pcp_title"
            app:layout_constraintTop_toBottomOf="@+id/device_name_title"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/peripheral_pcp_content"
            app:layout_constraintHorizontal_chainStyle="packed"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:onClick="onClick"/>

        <TextView
