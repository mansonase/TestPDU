                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_power_factor_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_power_factor_title"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_power_factor_title"
                app:layout_constraintEnd_toEndOf="parent"
                android:gravity="center"
                android:textColor="@color/black"
                android:text="0000.000"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_consumption_title"
                app:layout_constraintTop_toBottomOf="@+id/recorded_data_power_factor_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/recorded_data_consumption_content"
