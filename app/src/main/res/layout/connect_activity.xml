                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_current_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_current_title"
                app:layout_constraintStart_toEndOf="@+id/recorded_data_current_title"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="0000.000"
                android:gravity="center"
                android:textColor="@color/black"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_voltage_title"
                app:layout_constraintTop_toBottomOf="@+id/recorded_data_current_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/recorded_data_voltage_content"
                app:layout_constraintHorizontal_chainStyle="packed"
                android:gravity="center"
                android:textColor="@color/black"
                android:layout_marginTop="20dp"
                android:textSize="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/recorded_data_voltage_content"
                app:layout_constraintBottom_toBottomOf="@+id/recorded_data_voltage_title"
